@model PlanificatorProiecte.Models.Domain.StareAlocare
@{}

<div class="container pt-5">
    <div class="col-md-6">
        <form asp-action="Update" method="post">
            <input type="hidden" asp-for="Id"/>
            <div class="form-group">
                <h3>Modifica starea</h3>
                <label asp-for="NumeStareAlocare">Denumire stare</label>
                <input type="text" class="form-control" asp-for="NumeStareAlocare" />
                <span asp-validation-for="NumeStareAlocare" class="text-danger"></span>
            </div>



            @if (TempData["msg"] != null)
            {
                <div class="form-group">
                    @TempData["msg"]
                </div>
            }
            <div class="form-group pt-3">
            <button type="submit" class="btn btn-dark">Salveaza</button>
            <a href="/StareAlocare/GetAll" class="btn btn-primary">Lista</a>
            </div>
        </form>

    </div>


</div>

